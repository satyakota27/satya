files:
  "/opt/elasticbeanstalk/tasks/taillogs.d/rails-logs.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      /var/app/current/log/production.log

option_settings:
  aws:elasticbeanstalk:cloudwatch:logs:
    StreamLogs: true
    DeleteOnTerminate: false
    RetentionInDays: 7

